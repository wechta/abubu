<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:cookie_control/locallang_db.xml:cookie_control.pi_flexform.sheet_general
					</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
				 <consentModel>
        				<TCEforms>
         					<label>LLL:EXT:cookie_control/locallang_db.xml:cookie_control.pi_flexform.consentModel</label>
         						<config>
								<type>radio</type>
								<items type="array">
									<numIndex index="0" type="array">
									        <numIndex index="0">Information Only</numIndex>
									        <numIndex index="1">information_only</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
									        <numIndex index="0">Implicit</numIndex>
									        <numIndex index="1">implicit</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
									        <numIndex index="0">Explicit</numIndex>
									        <numIndex index="1">explicit</numIndex>
									</numIndex>
								</items>
								</config>
        				</TCEforms>
       		</consentModel>
					<themeTitle>
						<TCEforms>
							<label>LLL:EXT:cookie_control/locallang_db.xml:cookie_control.pi_flexform.theme_title</label>
							<config>
								<type>input</type>
								<size>30</size>
							</config>
						</TCEforms>
					</themeTitle>
					<introductory_text>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:cookie_control/locallang_db.xml:cookie_control.pi_flexform.introductory_text
							</label>
							<config>
								<type>text</type>
								<cols>30</cols>
								<rows>5</rows>
							</config>
						</TCEforms>
					</introductory_text>
         <additional_text>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:cookie_control/locallang_db.xml:cookie_control.pi_flexform.additional_text
							</label>
							<config>
								<type>text</type>
								<cols>30</cols>
								<rows>5</rows>
							</config>
						</TCEforms>
					</additional_text>
					<extra_options>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:cookie_control/locallang_db.xml:cookie_control.pi_flexform.extra_options
							</label>
							<config>
								<type>text</type>
								<cols>30</cols>
								<rows>5</rows>
							</config>
						</TCEforms>
					</extra_options>
					<countrySelection>
			        	<TCEforms>
			        		<label>LLL:EXT:cookie_control/locallang_db.xml:cookie_control.pi_flexform.theme_country_enable</label>
					 			<onChange>reload</onChange>
			         				<config>
			              			<type>check</type>
			         				</config>
			        	</TCEforms>
			    </countrySelection>
   				<countryField>
    				<TCEforms>
     					<label>LLL:EXT:cookie_control/locallang_db.xml:cookie_control.pi_flexform.theme_country_list</label>
 						<displayCond>FIELD:countrySelection:=:1</displayCond>
     						<config>
      							<type>input</type></config>
    				</TCEforms>
   				</countryField>
					<anlayticsCode>
        				<TCEforms>
         					<label>LLL:EXT:cookie_control/locallang_db.xml:cookie_control.pi_flexform.anlyticsCode</label>
         						<config>
          							<type>input</type></config>
        				</TCEforms>
       		</anlayticsCode>
       		
				</el>
			</ROOT>
		</sDEF>
		<display>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:cookie_control/locallang_db.xml:cookie_control.pi_flexform.theme_options</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
				<templateSelection>
						<TCEforms type="array">
                                    <config type="array">
                                        <type>input</type>
                                        <size>30</size>
                                        <max>256</max>
                                        <checkbox></checkbox>
                                        <eval>trim</eval>
                                        <wizards type="array">
                                            <_PADDING type="integer">2</_PADDING>
                                            <link type="array">
                                                <type>popup</type>
                                                <title>Link</title>
                                                <icon>link_popup.gif</icon>
                                                <script>browse_links.php?mode=wizard&amp;act=file</script>
                                                <JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
                                            </link>
                                        </wizards>
                                    </config>
                                   <label>LLL:EXT:cookie_control/locallang_db.xml:cookie_control.pi_flexform.file_upload</label>
                                </TCEforms>
       		</templateSelection>
       		<themeSelect>
						<TCEforms>
							<label>LLL:EXT:cookie_control/locallang_db.xml:cookie_control.pi_flexform.select_theme</label>
							<onChange>reload</onChange>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="5" type="array">
										<numIndex index="0">LLL:EXT:cookie_control/locallang_db.xml:cookie_control.pi_flexform.theme_default</numIndex>
										<numIndex index="1">0</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:cookie_control/locallang_db.xml:cookie_control.pi_flexform.theme_grey</numIndex>
										<numIndex index="1">1</numIndex>
									</numIndex>
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:cookie_control/locallang_db.xml:cookie_control.pi_flexform.theme_black</numIndex>
										<numIndex index="1">2</numIndex>
									</numIndex>
									<numIndex index="3" type="array">
									  <numIndex index="0">LLL:EXT:cookie_control/locallang_db.xml:cookie_control.pi_flexform.theme_blue</numIndex>
										<numIndex index="1">3</numIndex>
									</numIndex>
									<numIndex index="4" type="array">
										<numIndex index="0">LLL:EXT:cookie_control/locallang_db.xml:cookie_control.pi_flexform.theme_custom</numIndex>
										<numIndex index="1">4</numIndex>
									</numIndex>
								</items>
								<minitems>0</minitems>
								<maxitems>1</maxitems>
								<size>1</size>
							</config>
						</TCEforms>
					</themeSelect>
          <customThemeSelect>
						<TCEforms type="array">
                                    <config type="array">
                                        <type>input</type>
                                        <size>30</size>
                                        <max>256</max>
                                        <checkbox></checkbox>
                                        <eval>trim</eval>
                                        <wizards type="array">
                                            <_PADDING type="integer">2</_PADDING>
                                            <link type="array">
                                                <type>popup</type>
                                                <title>Link</title>
                                                <icon>link_popup.gif</icon>
                                                <script>browse_links.php?mode=wizard&amp;act=file</script>
                                                <JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
                                            </link>
                                        </wizards>
                                    </config>
                                    <label>LLL:EXT:cookie_control/locallang_db.xml:cookie_control.pi_flexform.upload_custom_theme</label>
                                    <displayCond>FIELD:themeSelect:=:4</displayCond>
                                </TCEforms>
       		</customThemeSelect>
       		<iconType>
						<TCEforms>
							<label>LLL:EXT:cookie_control/locallang_db.xml:cookie_control.pi_flexform.icon_type
							</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:cookie_control/locallang_db.xml:cookie_control.pi_flexform.diamond_view</numIndex>
										<numIndex index="0">Diamond</numIndex>
            							<numIndex index="1">diamon</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:cookie_control/locallang_db.xml:cookie_control.pi_flexform.triangle_view</numIndex>
										<numIndex index="0">Triangle</numIndex>
            							<numIndex index="1">triangle</numIndex>
									</numIndex>
								</items>
								<minitems>0</minitems>
								<maxitems>1</maxitems>
								<size>1</size>
							</config>
						</TCEforms>
					</iconType>
					<iconPosition>
						<TCEforms>
							<label>LLL:EXT:cookie_control/locallang_db.xml:cookie_control.pi_flexform.icon_position
							</label>
							<config>
								<type>select</type>
								<items type="array">
									<numIndex index="2" type="array">
										<numIndex index="0">LLL:EXT:cookie_control/locallang_db.xml:cookie_control.pi_flexform.icon_position_right</numIndex>
										<numIndex index="0">Right</numIndex>
            							<numIndex index="1">right</numIndex>
									</numIndex>
									<numIndex index="1" type="array">
										<numIndex index="0">LLL:EXT:cookie_control/locallang_db.xml:cookie_control.pi_flexform.icon_position_left</numIndex>
										<numIndex index="0">Left</numIndex>
            							<numIndex index="1">left</numIndex>
									</numIndex>
								</items>
								<minitems>0</minitems>
								<maxitems>1</maxitems>
								<size>1</size>
							</config>
						</TCEforms>
					</iconPosition>
					<enableSession>
			        	<TCEforms>
			        		<label>LLL:EXT:cookie_control/locallang_db.xml:cookie_control.pi_flexform.theme_enable_session</label>
			         				<config>
			              			<type>check</type>
			         				</config>
			        	</TCEforms>
			    </enableSession>
				</el>
			</ROOT>
		</display>
	</sheets>
</T3DataStructure>