<T3DataStructure>
    <meta>
       <langDisable>1</langDisable> 
	  <!--  <langChildren>1</langChildren>!-->
        </meta>
  <sheets>
	<sDEF>
	  <ROOT>
		<TCEforms>
			<sheetTitle>General</sheetTitle>
		</TCEforms>
	    <type>array</type>
	    <el>
		  <pidList>
		  	<TCEforms>
					<exclude>1</exclude>
					<label>Pid for database records</label>
					<config>
						<type>group</type>
						<internal_type>db</internal_type>
						<allowed>pages</allowed>
						<size>3</size>
						<maxitems>22</maxitems>
						<minitems>0</minitems>
						<show_thumbs>1</show_thumbs>
					</config>
		  	</TCEforms>
		  </pidList>
		  <pidThanks>
		  	<TCEforms>
					<exclude>1</exclude>
					<label>Pid for Thank you page</label>
					<config>
						<type>group</type>
						<internal_type>db</internal_type>
						<allowed>pages</allowed>
						<size>3</size>
						<maxitems>22</maxitems>
						<minitems>0</minitems>
						<show_thumbs>1</show_thumbs>
					</config>
		  	</TCEforms>
		  </pidThanks>
		  <pidLegal>
		  	<TCEforms>
					<exclude>1</exclude>
					<label>Pid for Legal notice</label>
					<config>
						<type>group</type>
						<internal_type>db</internal_type>
						<allowed>pages</allowed>
						<size>3</size>
						<maxitems>22</maxitems>
						<minitems>0</minitems>
						<show_thumbs>1</show_thumbs>
					</config>
		  	</TCEforms>
		  </pidLegal>
		  <templateFile>
				<TCEforms>
					<label>Path to template + template name:</label>
	         <config>
	          <type>input</type>
	          <size>30</size>
	         </config>
				</TCEforms>
      </templateFile>
      <sendToUser>
				<TCEforms>
					<label>Send email also to user</label>
					<config>
						<type>check</type>
						<default>1</default>
					</config>
				</TCEforms>
			</sendToUser>
	    </el>
	  </ROOT>
    </sDEF>
    <s_template>
	  <ROOT>
		<TCEforms>
			<sheetTitle>Mail setting:</sheetTitle>
		</TCEforms>
	    <type>array</type>
	    <el>
	    	
		    <subject>
					<TCEforms>
						<label>Subject:</label>
		         <config>
		          <type>input</type>
		          <size>30</size>
		         </config>
					</TCEforms>
	      </subject>
	      <subjectUser>
					<TCEforms>
						<label>Subject for user:</label>
		         <config>
		          <type>input</type>
		          <size>30</size>
		         </config>
					</TCEforms>
	      </subjectUser>
	      <fromEmail>
					<TCEforms>
						<label>From email:</label>
		         <config>
		          <type>input</type>
		          <size>30</size>
		         </config>
					</TCEforms>
	      </fromEmail>
	      <fromName>
					<TCEforms>
						<label>From name:</label>
		         <config>
		          <type>input</type>
		          <size>30</size>
		         </config>
					</TCEforms>
	      </fromName>
	      <toEmail>
					<TCEforms>
						<label>To email:</label>
		         <config>
		          <type>input</type>
		          <size>30</size>
		         </config>
					</TCEforms>
	      </toEmail>
	      <toName>
					<TCEforms>
						<label>To name:</label>
		         <config>
		          <type>input</type>
		          <size>30</size>
		         </config>
					</TCEforms>
	      </toName>
	    </el>
	  </ROOT>
    </s_template>
  </sheets>
</T3DataStructure>